	<entry key="login">
		SELECT
		       MEM_NO,
		       MEM_NAME,
		       MEM_PHONE,
		       MEM_ID,
		       MEM_PWD,
		       NICKNAME,
		       EMAIL,
		       BORN_DATE,
		       JOIN_DATE,
		       MEM_STATUS,
		       DELETE_YN
	      FROM
	           TB_MEMBER
	      JOIN
	      	   TB_NONMEM USING(MEM_NO)
	     WHERE
	           MEM_ID = ?
	       AND
	           MEM_PWD = ?
	       AND
	           DELETE_YN = 'Y' <!-- 회원 Y, 비회원 N -->
	       AND
	           MEM_STATUS = 'Y' <!-- 비회원/회원 중 회원 -->
	</entry>
	
