<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>
		Member Mapper
	</comment>
	
	
	<entry key="selectNonMemberList">
		
		SELECT
		        MEM_NO,
		        MEM_NAME,
		        MEM_PHONE
		  FROM
		        TB_NONMEM
		  WHERE
		        MEM_STATUS = 'N'
		 ORDER
		  	BY
		  		MEM_NO DESC
	
	</entry>

</properties>
