<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>
		MyPage Mapper
	</comment>
	
	<entry key="selectProfile">
		SELECT 
			   PROFILE_PATH 
		  FROM 
			   TB_PROFILE 
		 WHERE 
			   MEM_NO = ?
	</entry>
	
	<entry key="insertProfile">
		INSERT
		  INTO
		       TB_PROFILE
		VALUES
		       (
		       ?,
		       ?
		       )
	</entry>
	
	
</properties>
